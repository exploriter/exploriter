{
  "scripts": {
    "watch": "npx tailwindcss -i ./input.css -o ./output.css --watch",
    "prod": "npx tailwindcss -i ./input.css -o ./output.css --minify",
    "alpine": "npx esbuild ./input.js --bundle --minify --sourcemap --outfile=./output.js"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "alpinejs": "^3.14.0",
    "esbuild": "^0.21.4",
    "tippy.js": "^6.3.7"
  }
}
